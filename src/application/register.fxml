<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.register">
   <center>
      <AnchorPane prefHeight="409.0" prefWidth="378.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="BacktoLogin" layoutX="89.0" layoutY="154.0" mnemonicParsing="false" onAction="#LogIn" text="Login Page" />
            <Label layoutX="14.0" layoutY="28.0" text="Username:" />
            <TextField fx:id="registerUser" layoutX="14.0" layoutY="58.0" promptText="Username" />
            <Label layoutX="14.0" layoutY="92.0" text="Password:" />
            <PasswordField fx:id="registerPass" layoutX="14.0" layoutY="118.0" promptText="Password" />
            <Button fx:id="registerbutton" layoutX="21.0" layoutY="154.0" mnemonicParsing="false" onAction="#registerPerson" text="Register" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
