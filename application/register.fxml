<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="14.0" layoutY="28.0" text="Username:" />
            <TextField fx:id="registerUser" layoutX="14.0" layoutY="58.0" promptText="Username" />
            <Label layoutX="14.0" layoutY="92.0" text="Password:" />
            <PasswordField fx:id="registerPass" layoutX="14.0" layoutY="118.0" promptText="Password" />
            <Button fx:id="registerbutton" layoutX="63.0" layoutY="155.0" mnemonicParsing="false" onAction="#registerPerson" text="Register" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
