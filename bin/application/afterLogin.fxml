      <AnchorPane prefHeight="517.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="searchUni" layoutX="39.0" layoutY="25.0" mnemonicParsing="false" onAction="#University" text="Universities" />
            <Button fx:id="searchSchools" layoutX="158.0" layoutY="69.0" mnemonicParsing="false" onAction="#School" text="Schools" />
