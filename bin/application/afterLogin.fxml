<BorderPane prefHeight="523.0" prefWidth="415.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.afterLogin">
   <top>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="194.0" layoutY="83.0" text="Select from the buttons below:" />
         </children>
      </AnchorPane>
   </top>
   <right>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="322.0" fitWidth="201.0" layoutY="26.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../Downloads/smart-city-logo-icon-template-design-illustration-vector.jpg" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </right>
